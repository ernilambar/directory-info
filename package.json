{
  "name": "directory-info",
  "title": "Directory Info",
  "version": "1.0.2",
  "license": "GPL-2.0+",
  "main_file": "directory-info.php",
  "repository": {
    "type": "git",
    "url": "https://github.com/ernilambar/directory-info"
  },
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "prod": "NODE_ENV=production webpack",
    "eslint": "eslint --quiet .",
    "eslint:fix": "eslint --quiet --fix .",
    "format": "prettier --write \"src/**/*.scss\"",
    "preinstall": "npx only-allow pnpm",
    "version": "easy-replace-in-files",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules",
    "predeploy": "shx rm -rf vendor/ && composer install --no-dev --no-scripts -o",
    "deploy": "shx rm -rf deploy/ && shx mkdir deploy && copy-files-from-to && cd deploy/ && cross-var shx mv temp $npm_package_name && cross-var bestzip ../$npm_package_name.zip * && cd .. && cross-var shx mv $npm_package_name.zip deploy/"
  },
  "browserslist": "> 1%, not dead",
  "prettier": "@wordpress/prettier-config",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@wordpress/eslint-plugin": "^12.8.0",
    "@wordpress/prettier-config": "^1.4.0",
    "babel-loader": "^8.2.5",
    "bestzip": "^2.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-files-from-to": "^3.2.2",
    "cross-var": "^1.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "easy-replace-in-files": "^1.0.3",
    "eslint": "^8.21.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-wp-i18n": "^1.2.6",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "jquery-lazy": "^1.7.11",
    "toastify-js": "^1.12.0"
  }
}
